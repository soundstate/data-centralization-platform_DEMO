# Data Centralization Platform Environment Variables
# Copy this file to .env and fill in your actual values

# MusicBrainz API Configuration
# Required: Your application's User-Agent string (format: "AppName/Version ( contact-url )")
MUSICBRAINZ_USER_AGENT=YourApp/1.0.0 (https://your-contact-url.com)

# MusicBrainz OAuth 2.0 Configuration
# Required for OAuth: Get these from https://musicbrainz.org/account/applications
MUSICBRAINZ_CLIENT_ID=your_oauth_client_id_here
MUSICBRAINZ_CLIENT_SECRET=your_oauth_client_secret_here
MUSICBRAINZ_REDIRECT_URI=http://localhost:8000/auth/musicbrainz/callback

# MusicBrainz API Settings
MUSICBRAINZ_RATE_LIMIT_PER_SECOND=1.0
MUSICBRAINZ_DEBUG_MODE=false

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/data_centralization
POSTGRES_PASSWORD=postgres

# Redis Configuration (for OAuth session storage)
REDIS_URL=redis://localhost:6379/0

# Development Settings
DEBUG_MODE=true
LOG_LEVEL=INFO
ENVIRONMENT=development
